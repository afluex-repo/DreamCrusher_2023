@model DreamCrusher.Models.Reports

@{
    ViewBag.Title = "UnPaidIncomes";
    Layout = "~/Views/Shared/_Layout.cshtml";
}
       <div class="page-content">
    <div class="row">
        <div class="col-12 col-lg-12 col-xl-12">
            <div class="card">
                <div class="card-body">
                    <div class="d-flex align-items-center justify-content-between">
                        <h5 class="mb-0">UnPaid Incomes</h5>
                    </div>
                </div>
            </div>
        </div>

        <div class="col-12 col-lg-12 col-xl-12">
            <div class="card">
                <div class="card-body">
                    <div class="table-responsive">
                        <table id="example2" class="table table-striped table-bordered">
                            <thead>
                                <tr>
                                    <th>Sr No.</th>
                                    <th>Date</th>
                                    <th>From LoginId</th>
                                    <th>From Name</th>
                                    <th>Amount</th>
                                    <th>Income Type</th>
                                </tr>
                            </thead>
                            <tbody>
                                @if (Model != null)
                                {
                                    if (Model.lstunpaidincomes != null)
                                    {
                                        int i = 1;
                                        foreach (var Item in Model.lstunpaidincomes)
                                        {
                                            <tr class="text-left">
                                                <td>@i</td>
                                                <td>@Item.Date</td>
                                                <td>@Item.FromLoginId</td>
                                                <td>@Item.FromUserName</td>

                                                <td>@Item.Amount</td>

                                                <td>@Item.IncomeType</td>

                                            </tr>
                                            i++;
                                        }

                                    }
                                }
                            </tbody>
                        </table>
                    </div>
                </div>
            </div>
        </div>
    </div>
</div>



