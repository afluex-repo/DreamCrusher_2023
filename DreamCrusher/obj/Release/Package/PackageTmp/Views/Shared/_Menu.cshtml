@model DreamCrusher.Models.Home

<ul class="metismenu" id="menu">
    <li>
        <a href="/Admin/AdminDashBoard">
            <div class="parent-icon">
                <i class='bx bx-home-alt'></i>
            </div>
            <div class="menu-title">Dashboard</div>
        </a>
    </li>
    @if (Model != null)
            {
                if (Model.lstMenu != null)
                {
                    foreach (var item in Model.lstMenu)
                    {
                <li>
                    <a href="javascript:;" class="has-arrow">
                        <div class="parent-icon">
                            <i class="@item.Icon"></i>
                        </div>
                        <div class="menu-title">@item.MenuName</div>
                    </a>
                    @if (item.Url == "#")
                    {
                        <ul>
                            @foreach (var item1 in Model.lstsubmenu)
                            {
                                if (item1.MenuId == item.MenuId)
                                {
                                    <li>
                                        <a href="@item1.Url"><i class='bx bx-radio-circle'></i>@item1.SubMenuName</a>
                                    </li>
                                }
                            }
                        </ul>
                    }
                </li>
            }
        }
    }
</ul>









