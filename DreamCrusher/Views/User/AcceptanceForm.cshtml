@model DreamCrusher.Models.DashBoard

@{
    Layout = null;
}
<!doctype html>
<html lang="en">
<head>
    <!-- Required meta tags -->
    <meta charset="utf-8">
    <meta name="viewport" content="width=device-width, initial-scale=1">
    <!--favicon-->
    <link rel="icon" href="~/assetsAdmin/images/favicon-32x32.png" type="image/png" />
    <!--plugins-->
    <link href="~/assetsAdmin/plugins/vectormap/jquery-jvectormap-2.0.2.css" rel="stylesheet" />
    <link href="~/assetsAdmin/plugins/simplebar/css/simplebar.css" rel="stylesheet" />
    <link href="~/assetsAdmin/plugins/perfect-scrollbar/css/perfect-scrollbar.css" rel="stylesheet" />
    <link href="~/assetsAdmin/plugins/metismenu/css/metisMenu.min.css" rel="stylesheet" />
    <link href="~/assetsAdmin/plugins/datatable/css/dataTables.bootstrap5.min.css" rel="stylesheet" />
    <!-- loader-->
    @*<link href="~/assetsAdmin/css/pace.min.css" rel="stylesheet" />*@
    <script src="~/assetsAdmin/js/pace.min.js"></script>
    <!-- Bootstrap CSS -->
    <link href="~/assetsAdmin/css/bootstrap.min.css" rel="stylesheet">
    <link href="~/assetsAdmin/css/bootstrap-extended.css" rel="stylesheet">
    <link href="https://fonts.googleapis.com/css2?family=Roboto:wght@400;500&amp;display=swap" rel="stylesheet">
    <link href="~/assetsAdmin/css/app.css" rel="stylesheet">
    <link href="~/assetsAdmin/css/icons.css" rel="stylesheet">
    <title>@ViewBag.Title</title>
    <style>
        @@media print {
            .logod img {
                width: 250px;
                height: auto;
                margin: 10px auto;
                display: block;
            }
            .logod {
                margin-bottom: 10px;
            }
            #download {
                     display: none;
            }
        }
        .logod img {
                width: 350px;
                height: auto;
                margin: 10px auto;
                display: block;
            }
            .logod {
                margin-bottom: 10px;
            }
    </style>
</head>
<body class="bg-theme bg-theme1">
    <div class="main-file">
        <div class="container" id="printableArea">
            <div class="row">
                <div class="col-md-12 col-lg-12 col-sm-12">
                    <div class="logod">
                        <img src="/Websiteassets/images/logo-white.png"  class="img-fluid" alt="logo" />
                        <button id="download" class="text-end btn btn-light px-4 my-4" onclick="javascript: window.print();">Download Pdf</button>
                    </div>
                   
                </div>
                <div class="col-md-6 col-lg-6 col-sm-12">
                    <div class="logo-detail">
                        <h4>ACCEPTANCE FORM</h4>
                        <p>ACCEPTANCE of ONLINE AGREEMENT / CONTRACT with Dream Crushers Pvt. Ltd</p>
                    </div>
                </div>
                <div class="col-md-6 col-lg-6 col-sm-12">
                    <div class="logo-detail">
                        <h4>Dream Crushers PVT. LTD.</h4>
                        <p>
                            Devkali Bypass, Lucknow Road, Ayodhya, Uttar Pradesh, 224001, India
                        </p>
                    </div>
                </div>
            </div>

            <div class="row">
                <div class="col-12 col-lg-12 col-xl-12">
                    <div class="card">
                        <div class="card-body">
                            <div class="table-responsive">
                                <table class="table table-striped table-bordered">
                                    <thead>
                                        <tr>
                                            <th>User ID:</th>
                                            <th>Name:</th>
                                            <th>Permanent Address:</th>
                                            <th>Contact No.:</th>
                                            <th> Registration Date:</th>
                                            <th>  Introducing Dream Crushers Representative (Sponsor):</th>
                                        </tr>
                                    </thead>
                                    <tbody>
                                        <tr class="text-left">
                                            <td>@ViewBag.LoginId</td>
                                            <td>@ViewBag.DisplayName</td>
                                            <td>@ViewBag.Address  </td>
                                            <td>@ViewBag.Mobile</td>
                                            <td>@ViewBag.JoiningDate</td>
                                            <td>@ViewBag.SponsorId</td>
                                        </tr>
                                    </tbody>
                                </table>
                            </div>
                        </div>
                    </div>
                </div>
                @*<div class="col-md-12 col-lg-12 col-sm-12">
                    <form>
                        <div class="form-row">
                            <div class="col-md-2">
                                User ID:
                            </div>
                            <div class="col-md-4" style="border:1px;border-bottom:solid">
                                @ViewBag.LoginId
                            </div>

                            <div class="col-md-2">
                                Name:
                            </div>
                            <div class="col-md-4" style="border:1px;border-bottom:solid">
                                @ViewBag.DisplayName
                            </div>
                        </div>
                        <div class="form-row">
                            <div class="col-md-2">
                                Permanent Address:
                            </div>
                            <div class="col-md-4" style="border:1px;border-bottom:solid">
                                @ViewBag.Address
                            </div>

                            <div class="col-md-2">
                                Contact No.:
                            </div>
                            <div class="col-md-4" style="border:1px;border-bottom:solid">
                                @ViewBag.Mobile
                            </div>




                        </div>
                        <div class="form-row">
                            <div class="col-md-2">
                                Registration Date:
                            </div>
                            <div class="col-md-4" style="border:1px;border-bottom:solid">
                                @ViewBag.JoiningDate
                            </div>
                            <div class="col-md-2">
                                Introducing Dream Crushers Representative
                                (Sponsor):
                            </div>
                            <div class="col-md-4" style="border:1px;border-bottom:solid">
                                @ViewBag.SponsorId
                            </div>

                        </div>
                    </form>
                </div>*@
            </div>
            <br />
            <div class="row">
                <div class="col-md-12 col-lg-12 col-sm-12">
                    <div class="declaration">
                        <h4 class="text-center">DECLARATION</h4>
                        <p><strong>I solemnly declare and affirm as under –</strong></p>
                        <p>1. I declare that I am of the age of 18 years or more and competent to contract</p>
                        <p>
                            2. That I have read and understood the terms and conditions for appointment of Dream Crushers
                            Retailer / Representative (Direct Seller) of the Company, and accept to them.
                        </p>
                        <p>
                            3. I have also gone through the Company's official website including FAQs, Printed materials
                            and Brochures. I agree to the contents of the materials and convinced about the business and
                            I have applied to appoint me as a Direct Seller on my own volition.
                        </p>
                        <p>
                            4. I declare that I have not been given any assurance or promise or inducement by the Company
                            or its Directors in regard to any fixed income incentive, prize or benefit on account of the
                            products purchased by me.
                        </p>
                        <p>
                            5. I have clearly understood that eligibility of income exclusively depends on my performance
                            as per the Business Plan of the company. I further agree that company reserves the right to
                            change the Business Plan at any point of time without any prior notice.
                        </p>
                        <p>6. I undertake not to misguide or induce dishonestly anybody to join the Company.</p>
                        <p>
                            7. I hereby agree to submit all disputes to Dispute Redressal cell / Arbitration as provided
                            in the terms and conditions of the Company.
                        </p>
                        <p>
                            8. I hereby declare that I am signing this “DECLARATION” with complete understanding and with
                            my FREE WILL, without any PRESSURE / UNDUE INFLUENCE or INDUCEMENT.
                        </p>
                        <p>
                            9. I hereby agree and adhere to the terms and conditions as given on the website of the
                            company (www.dreamcrushers.in) and as mentioned above to purchase the product and to do the
                            Direct Seller activities.
                        </p>
                    </div>
                </div>

            </div>

            <div class="row">
                <div class="col-md-12 col-lg-12 col-sm-12">
                    <div class="tablesd">
                        <table class="table table-bordered">
                            <tbody>
                                <tr>
                                    <td class="text-center">
                                        Signatures of Joining Direct Seller<br>(Dream Crushers
                                        Retailer)
                                    </td>
                                    <td class="text-center">
                                        Signatures of Sponsoring Dream Crushers Representative<br>
                                        (WITNESS)
                                    </td>
                                </tr>
                            </tbody>
                        </table>
                    </div>
                </div>

            </div>

            <div class="row">
                <div class="col-md-12 col-lg-12 col-sm-12">
                    <div class="following-documents">
                        <h4>Please send this form along with following documents to the company’s office</h4>
                        <p>
                            1. A photocopy of any one of the self-attested Govt. issued photo Identification documents.
                        </p>
                        <p>2. A photocopy of any one of the self-attested Govt. issued documents for address proof.</p>
                        <p>3. Self-attested Photocopy of PAN card.</p>
                    </div>
                </div>
            </div>

            <div class="row">
                <div class="col-md-12 col-lg-12 col-sm-12">
                    <div class="note">
                        <p>
                            <strong>NOTE :</strong> It is the sole responsibility of the joining Dream Crushers Retailer to submit authentic
                            documents and information, any discrepancy or any civil/criminal liability arising due to
                            this regard would be the full and complete responsibility of the said joining Dream Crushers
                            Retailer. The company shall not be responsible in this regard in any way
                        </p>
                    </div>
                </div>
            </div>
        </div>

    </div>

    <!-- Optional JavaScript -->
    <!-- jQuery first, then Popper.js, then Bootstrap JS -->
    @*<script src="https://code.jquery.com/jquery-3.5.1.slim.min.js"
                integrity="sha384-DfXdz2htPH0lsSSs5nCTpuj/zy4C+OGpamoFVy38MVBnE+IbbVYUew+OrCXaRkfj"
                crossorigin="anonymous"></script>
        <script src="https://cdn.jsdelivr.net/npm/popper.js@1.16.1/dist/umd/popper.min.js"
                integrity="sha384-9/reFTGAW83EW2RDu2S0VKaIzap3H66lZH81PoYlFhbGU+6BZp6G7niu735Sk7lN"
                crossorigin="anonymous"></script>
        <script src="../assets/AcceptanceCSS/js/bootstrap.min.js"></script>*@
</body>


</html>

