@model DreamCrusher.Models.DashBoard
@{
    ViewBag.Title = "AdminDashBoard";
    Layout = "~/Views/Shared/_AdminLayout.cshtml";
}


<style>
    .tab1{
        background-color:#902a8e;
    }
      .tab2{
           background-color:#38a39d;
    }
        .tab3{
             background-color:#832d38;
    }
     
       

</style>



<div class="page-content">
    <div class="row">
        <div class="col-12 col-lg-12 col-xl-12">
            <div class="card">
                <div class="card-body">
                    <h4 class="mb-0">Dashboard</h4>
                </div>
            </div>
        </div>
    </div>
    <div class="row row-cols-1 row-cols-md-2 row-cols-lg-2 row-cols-xl-3">


        <div class="col">
            <div class="card radius-10" style="background:#feefe8">
                <div class="card-body">
                    <div class="card mb-2" style="background:#fe7840;">
                        <div class="card-body p-0">
                            <div class="d-flex align-items-center">
                                <div class="widgets-icons widgets-icons-custom bg-light-transparent text-white">
                                    <i class="bx bx-user"></i>
                                </div>
                                <div class="widgets-icons widgets-icons-custom bg-light-transparent text-white ms-auto">
                                    <i class="bx bx-dots-vertical-rounded"></i>
                                </div>
                            </div>
                        </div>
                    </div>
                    <div class="d-flex align-items-center text-center justify-content-center">
                        <div>
                            <h5 class="mb-2 text-dark"><i class="bx bx-user"></i> @ViewBag.TotalUsers</h5>
                            <h5 class="mb-0 text-dark">Total Associates</h5>
                        </div>
                    </div>
                </div>
            </div>
        </div>
        <div class="col">
            <div class="card radius-10" style="background:#e4f9fc;">
                <div class="card-body">
                    <div class="card mb-2" style="background:#0cc9da;">
                        <div class="card-body p-0">
                            <div class="d-flex align-items-center">
                                <div class="widgets-icons widgets-icons-custom bg-light-transparent text-white">
                                    <i class="bx bx-user"></i>
                                </div>
                                <div class="widgets-icons widgets-icons-custom bg-light-transparent text-white ms-auto">
                                    <i class="bx bx-dots-vertical-rounded"></i>
                                </div>
                            </div>
                        </div>
                    </div>
                    <div class="d-flex align-items-center text-center justify-content-center">
                        <div>
                            <h5 class="mb-2 text-dark"><i class="bx bx-user"></i> @ViewBag.ActiveUsers</h5>
                            <h5 class="mb-0 text-dark">Active Users</h5>
                        </div>
                    </div>
                </div>
            </div>
        </div>
        <div class="col">
            <div class="card radius-10" style="background:#fee6f3">
                <div class="card-body">
                    <div class="card mb-2" style="background:#fd3c95;">
                        <div class="card-body p-0">
                            <div class="d-flex align-items-center">
                                <div class="widgets-icons widgets-icons-custom bg-light-transparent text-white">
                                    <i class="bx bx-block"></i>
                                </div>
                                <div class="widgets-icons widgets-icons-custom bg-light-transparent text-white ms-auto">
                                    <i class="bx bx-dots-vertical-rounded"></i>
                                </div>
                            </div>
                        </div>
                    </div>
                    <div class="d-flex align-items-center text-center justify-content-center">
                        <div>
                            <h5 class="mb-2 text-dark"><i class="bx bx-user"></i> @ViewBag.BlockedUsers</h5>
                            <h5 class="mb-0 text-dark">Blocked Users</h5>
                        </div>
                    </div>
                </div>
            </div>
        </div>
        <div class="col">
            <div class="card radius-10" style="background:#e4f9fc">
                <div class="card-body">
                    <div class="card mb-2" style="background:#6d81f4;">
                        <div class="card-body p-0">
                            <div class="d-flex align-items-center">
                                <div class="widgets-icons widgets-icons-custom bg-light-transparent text-white">
                                    <i class="bx bx-rupee"></i>
                                </div>
                                <div class="widgets-icons widgets-icons-custom bg-light-transparent text-white ms-auto">
                                    <i class="bx bx-dots-vertical-rounded"></i>
                                </div>
                            </div>
                        </div>
                    </div>
                    <div class="d-flex align-items-center text-center justify-content-center">
                        <div>
                            <h5 class="mb-2 text-dark"><i class="bx bx-rupee"></i> @ViewBag.TodayIncome</h5>
                            <h5 class="mb-0 text-dark">Today Income</h5>
                        </div>
                    </div>
                </div>
            </div>
        </div>
        <div class="col">
            <div class="card radius-10" style="background:#f1e9f9">
                <div class="card-body">
                    <div class="card mb-2" style="background:#8241c6;">
                        <div class="card-body p-0">
                            <div class="d-flex align-items-center">
                                <div class="widgets-icons widgets-icons-custom bg-light-transparent text-white">
                                    <i class="bx bx-rupee"></i>
                                </div>
                                <div class="widgets-icons widgets-icons-custom bg-light-transparent text-white ms-auto">
                                    <i class="bx bx-dots-vertical-rounded"></i>
                                </div>
                            </div>
                        </div>
                    </div>
                    <div class="d-flex align-items-center text-center justify-content-center">
                        <div>
                            <h5 class="mb-2 text-dark"><i class="bx bx-rupee"></i> @ViewBag.LastSevenDaysIncome</h5>
                            <h5 class="mb-0 text-dark">Last 7 Days Income</h5>
                        </div>
                    </div>
                </div>
            </div>
        </div>
        <div class="col">
            <div class="card radius-10" style="background:#eef7f2">
                <div class="card-body">
                    <div class="card mb-2" style="background:#3d6c54;">
                        <div class="card-body p-0">
                            <div class="d-flex align-items-center">
                                <div class="widgets-icons widgets-icons-custom bg-light-transparent text-white">
                                    <i class="bx bx-rupee"></i>
                                </div>
                                <div class="widgets-icons widgets-icons-custom bg-light-transparent text-white ms-auto">
                                    <i class="bx bx-dots-vertical-rounded"></i>
                                </div>
                            </div>
                        </div>
                    </div>
                    <div class="d-flex align-items-center text-center justify-content-center">
                        <div>
                            <h5 class="mb-2 text-dark"><i class="bx bx-rupee"></i> @ViewBag.LastThirtyDaysIncome</h5>
                            <h5 class="mb-0 text-dark">Last 30 Days Income</h5>
                        </div>
                    </div>
                </div>
            </div>
        </div>
        <div class="col">
            <div class="card radius-10" style="background:#feefe8">
                <div class="card-body">
                    <div class="card mb-2" style="background:#2e77c9;">
                        <div class="card-body p-0">
                            <div class="d-flex align-items-center">
                                <div class="widgets-icons widgets-icons-custom bg-light-transparent text-white">
                                    <i class="bx bx-rupee"></i>
                                </div>
                                <div class="widgets-icons widgets-icons-custom bg-light-transparent text-white ms-auto">
                                    <i class="bx bx-dots-vertical-rounded"></i>
                                </div>
                            </div>
                        </div>
                    </div>
                    <div class="d-flex align-items-center text-center justify-content-center">
                        <div>
                            <h5 class="mb-2 text-dark"><i class="bx bx-rupee"></i> @ViewBag.TotalIncome</h5>
                            <h5 class="mb-0 text-dark">Total Income</h5>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </div>
    <div class="row">
        <div class="col-12 col-lg-4 col-xl-3 d-flex align-items-center">
            <div class="card radius-10 overflow-hidden w-100">
                <div class="card-body">
                    <p class="mb-0 text-white">Associate Details</p>
                    <div id="divusers"></div>
                </div>
            </div>
        </div>
        <div class="col-12 col-lg-4 col-xl-3 d-flex align-items-center">
            <div class="card radius-10 overflow-hidden w-100">
                <div class="card-body">
                    <p class="mb-0 text-white">Payout Status</p>
                    <div id="divpayout"></div>
                </div>
            </div>
        </div>
        <div class="col-12 col-lg-4 col-xl-6 d-flex align-items-center">
            <div class="card radius-10 overflow-hidden w-100">
                <div class="card-body">
                    <p class="mb-0 text-white">Business Details</p>
                    <div id="divinvestment"></div>
                </div>
            </div>
        </div>
        <div class="col-12 col-lg-4 col-xl-6 d-flex align-items-center">
            <div class="card radius-10 overflow-hidden w-100">
                <div class="card-body">
                    <p class="mb-0 text-white">Joining Details</p>
                    <div id="divjoining"></div>
                </div>
            </div>
        </div>
    </div>
    <!--End Row-->
</div>
<script type="text/javascript" src="https://www.gstatic.com/charts/loader.js"></script>



