@model DreamCrusher.Models.EmployeeRegistrations

@{
    ViewBag.Title = "EmployeeDetails";
    Layout = "~/Views/Shared/_AdminLayout.cshtml";
}

@using (Html.BeginForm("PayoutReport", "AdminReport", FormMethod.Post, new { @id = "form1" }))
{

    <div class="page-content">
        <div class="row">
            <div class="col-12 col-lg-12 col-xl-12">
                <div class="card">
                    <div class="card-body">
                        <div class="d-flex align-items-center justify-content-between">
                            <h5 class="mb-0">Employee Details</h5>
                            <a class="btn btn-light repeater-add-btn px-4" href="/EmployeeRegistration/EmployeeRegistration">Add New</a>
                        </div>
                    </div>
                </div>
            </div>
            <div class="col-12 col-lg-12 col-xl-12">
                <div class="card">
                    <div class="card-body">
                        <div class="table-responsive">
                            <table id="example2" class="table table-striped table-bordered">
                                <thead>
                                    <tr>
                                        <th>Sr No.</th>
                                        <th>Name</th>
                                        <th> Login Id</th>
                                        <th>Qualififcation</th>
                                        <th>Contact No</th>
                                        <th>Email Id</th>


                                    </tr>
                                </thead>
                                <tbody>
                                    @if (Model != null)
                                    {
                                        if (Model.lstemp != null)
                                        {
                                            int i = 1;
                                            foreach (var item in Model.lstemp)
                                            {

                                                <tr>
                                                    <td>@i</td>
                                                    <td>@Html.DisplayFor(m => item.Name)</td>
                                                    <td>@Html.DisplayFor(m => item.LoginId)</td>
                                                    <td>@Html.DisplayFor(m => item.EducationQualififcation)</td>

                                                    <td>@Html.DisplayFor(m => item.Mobile)</td>
                                                    <td>@Html.DisplayFor(m => item.Email)</td>

                                                </tr>


                                                i++;
                                            }
                                        }
                                    }
                                </tbody>
                            </table>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </div>



}


